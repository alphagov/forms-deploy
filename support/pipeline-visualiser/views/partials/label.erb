<% case status
   when "Cancelled" %>
    <strong class="govuk-tag govuk-tag--purple">
        Cancelled
    </strong>
<% when "InProgress" %>
    <strong class="govuk-tag govuk-tag--yellow">
        In Progress
    </strong>
<% when "Failed" %>
    <strong class="govuk-tag govuk-tag--red">
        Failed
    </strong>
<% when "Stopped" %>
    <strong class="govuk-tag govuk-tag--blue">
        Stopped
    </strong>
<% when "Stopping" %>
    <strong class="govuk-tag govuk-tag--turqoise">
        Stopping
    </strong>
<% when "Succeeded" %>
    <strong class="govuk-tag govuk-tag--green">
        Succeeded
    </strong>
<% when "Not Started" %>
    <strong class="govuk-tag govuk-tag--grey">
        Not Started
    </strong>
<% when "Paused" # This is one of our own states, not one of CodePipelines %>
    <strong class="govuk-tag govuk-tag--light-blue">
        Paused
    </strong>
<% when "InboundTransitionPaused" # This is one of our own states, not one of CodePipelines %>
    <strong class="govuk-tag govuk-tag--light-blue">
        Paused On Inbound
    </strong>
<% end %>
